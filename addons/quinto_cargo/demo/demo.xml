        <record id="mudanza_tech" model="quintocargo.mudanzas">
            <field name="cod_mudanza">1001</field>
            <field name="nombre">Traslado Oficina Tech</field>
            <field name="telefono">912345678</field>
            <field name="fecha_recogida">2024-06-01 08:00:00</field>
            <field name="fecha_entrega">2024-06-05 18:00:00</field>
            <field name="direccion_origen">Calle Serrano 45, Madrid</field>
            <field name="direccion_destino">Calle Gran Vía 100, Madrid</field>
            <field name="precio">1200.0</field>
            <field name="estado">almacenado</field>
            <field name="metros_cubicos_usados">85.0</field>
            <field name="almacen_id" ref="almacen_central"/>
            <field name="cliente_id" ref="cliente_empresa1"/>
            <field name="empleado_ids" eval="[(4, ref('empleado_juan')), (4, ref('empleado_maria'))]"/>
            <field name="bienes_ids" eval="[(4, ref('sofá_antiguo')), (4, ref('tv_oled'))]"/>
