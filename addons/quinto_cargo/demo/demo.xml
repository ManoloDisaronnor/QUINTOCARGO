<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Datos para Almacén -->
    <data>
        <!-- Almacenes -->
        <record id="almacen_central" model="quintocargo.almacen">
            <field name="nombre">Almacén Central</field>
            <field name="direccion">Calle Mayor 123, Madrid</field>
            <field name="capacidad">500.0</field>
        </record>

        <record id="almacen_norte" model="quintocargo.almacen">
            <field name="nombre">Almacén Norte</field>
            <field name="direccion">Avenida Cantabria 45, Santander</field>
            <field name="capacidad">300.0</field>
        </record>

        <record id="almacen_sur" model="quintocargo.almacen">
            <field name="nombre">Almacén Sur</field>
            <field name="direccion">Plaza Andalucía 7, Sevilla</field>
            <field name="capacidad">400.0</field>
        </record>

        <record id="almacen_este" model="quintocargo.almacen">
            <field name="nombre">Almacén Este</field>
            <field name="direccion">Paseo Marítimo 22, Valencia</field>
            <field name="capacidad">350.0</field>
        </record>

        <!-- Datos para Bienes Asegurados -->
        <record id="sofá_antiguo" model="quintocargo.bien_asegurado">
            <field name="nombre">Sofá Antiguo</field>
            <field name="valor">1500.0</field>
            <field name="tipo_bien">mueble</field>
            <field name="peso">85.5</field>
            <field name="dimensiones_Alto">1.0</field>
            <field name="dimensiones_Ancho">2.4</field>
            <field name="dimensiones_Largo">0.9</field>
        </record>

        <record id="tv_oled" model="quintocargo.bien_asegurado">
            <field name="nombre">TV OLED 85"</field>
            <field name="valor">3500.0</field>
            <field name="tipo_bien">electrodomestico</field>
            <field name="peso">32.0</field>
            <field name="dimensiones_Alto">1.2</field>
            <field name="dimensiones_Ancho">0.2</field>
            <field name="dimensiones_Largo">1.9</field>
        </record>

        <record id="cuadro_picasso" model="quintocargo.bien_asegurado">
            <field name="nombre">Cuadro de Picasso</field>
            <field name="valor">250000.0</field>
            <field name="tipo_bien">arte</field>
            <field name="peso">8.5</field>
            <field name="dimensiones_Alto">0.8</field>
            <field name="dimensiones_Ancho">0.6</field>
            <field name="dimensiones_Largo">0.1</field>
        </record>

        <record id="armario_roble" model="quintocargo.bien_asegurado">
            <field name="nombre">Armario de Roble</field>
            <field name="valor">4500.0</field>
            <field name="tipo_bien">mueble</field>
            <field name="peso">120.0</field>
            <field name="dimensiones_Alto">2.2</field>
            <field name="dimensiones_Ancho">1.5</field>
            <field name="dimensiones_Largo">0.6</field>
        </record>


        <!-- Datos para Clientes -->
        <record id="cliente_empresa1" model="quintocargo.cliente">
            <field name="dni">12345678A</field>
            <field name="foto"></field>            <!-- Dejamos la etiqueta field para la foto, pero sin imagen -->
            <field name="nombre">Empresa</field>
            <field name="apellidos">Ejemplo S.L.</field>
            <field name="direccion">Calle Falsa 123, Madrid</field>
            <field name="telefono">912345678</field>
            <field name="email">empresa@ejemplo.com</field>
            <field name="fecha_nacimiento">1990-01-01</field>
            <field name="cuenta_corriente">ES9121000418450200051332</field>
            <field name="tipoCliente">empresa</field>
        </record>

        <record id="empleado_juan" model="quintocargo.empleado">
            <field name="dni">87654321B</field>
            <field name="foto"></field>            <!-- Dejamos la etiqueta field para la foto, pero sin imagen -->
            <field name="nombre">Juan</field>
            <field name="apellidos">Pérez García</field>
            <field name="direccion">Avenida Siempre Viva 742, Springfield</field>
            <field name="telefono">623456789</field>
            <field name="email">juan.perez@ejemplo.com</field>
            <field name="fecha_nacimiento">1985-05-15</field>
            <field name="cuenta_corriente">ES7621000418450200051332</field>
            <field name="cargo">empleado</field>
            <field name="fecha_contratacion">2015-06-01</field>
            <field name="sueldo">1500.00</field>
            <field name="horas_trabajadas">160.00</field>
        </record>

        <!-- Datos para Mudanzas -->

        <record id="mudanza_tech" model="quintocargo.mudanzas">
            <field name="cod_mudanza">1001</field>
            <field name="nombre">Traslado Oficina Tech</field>
            <field name="telefono">912345678</field>
            <field name="fecha_recogida">2024-06-01 08:00:00</field>
            <field name="fecha_entrega">2024-06-05 18:00:00</field>
            <field name="direccion_origen">Calle Serrano 45, Madrid</field>
            <field name="direccion_destino">Calle Gran Vía 100, Madrid</field>
            <field name="precio">1200.0</field>
            <field name="estado">almacenado</field>
            <field name="metros_cubicos_usados">85.0</field>
            <field name="almacen_id" ref="almacen_central"/>
            <field name="cliente_id" ref="cliente_empresa1"/>
            <field name="empleado_ids" eval="[(4, ref('empleado_juan')), (4, ref('empleado_maria'))]"/>
            <field name="bienes_ids" eval="[(4, ref('sofá_antiguo')), (4, ref('tv_oled'))]"/>
        </record>

        <record id="mudanza_ana" model="quintocargo.mudanzas">
            <field name="cod_mudanza">1002</field>
            <field name="nombre">Mudanza Familiar García</field>
            <field name="telefono">676543210</field>
            <field name="fecha_recogida">2024-06-10 09:00:00</field>
            <field name="fecha_entrega">2024-06-12 17:00:00</field>
            <field name="direccion_origen">Avenida Libertad 3, Barcelona</field>
            <field name="direccion_destino">Calle Rosales 12, Valencia</field>
            <field name="precio">850.0</field>
            <field name="estado">pendiente</field>
            <field name="metros_cubicos_usados">45.0</field>
            <field name="almacen_id" ref="almacen_norte"/>
            <field name="cliente_id" ref="cliente_particular1"/>
            <field name="empleado_ids" eval="[(4, ref('empleado_pedro'))]"/>
        </record>


        <!-- Datos para Transportes -->

        <record id="transporte_camion1" model="quintocargo.transporte">
            <field name="matricula">1234ABC</field>
            <field name="capacidad">2000.0</field>
            <field name="tipo_vehiculo">camion</field>
            <field name="fecha_compra">2023-05-20</field>
            <field name="fecha_itv">2025-05-20</field>
        </record>

        <record id="transporte_furgoneta1" model="quintocargo.transporte">
            <field name="matricula">5678DEF</field>
            <field name="capacidad">800.0</field>
            <field name="tipo_vehiculo">furgoneta</field>
            <field name="fecha_compra">2024-01-10</field>
            <field name="fecha_itv">2026-01-10</field>
        </record>
    </data>
</odoo>